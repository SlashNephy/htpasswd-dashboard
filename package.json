{
  "name": "htpasswd-dashboard",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/htpasswd-dashboard"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "hadolint": "hadolint Dockerfile",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "start": "next start"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@kubernetes/client-node": "0.18.1",
    "@mantine/core": "5.10.5",
    "@mantine/hooks": "5.10.5",
    "@mantine/next": "5.10.5",
    "@mantine/notifications": "5.10.5",
    "@tabler/icons-react": "2.30.0",
    "@tanstack/react-query": "4.32.6",
    "async-mutex": "0.4.0",
    "bcrypt": "5.1.0",
    "gravatar": "1.8.2",
    "http-status-codes": "2.2.0",
    "jsonwebtoken": "9.0.1",
    "jwks-rsa": "3.0.1",
    "nanoid": "4.0.2",
    "next": "13.4.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-array-length": "0.1.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "1.0.6",
    "@slashnephy/prettier-config": "0.1.27",
    "@types/bcrypt": "5.0.0",
    "@types/gravatar": "1.8.3",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "18.17.5",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "concurrently": "8.2.0",
    "eslint": "8.47.0",
    "prettier": "3.0.2",
    "typescript": "5.1.6"
  },
  "packageManager": "yarn@3.6.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ],
    "rules": {
      "import/extensions": "off"
    }
  }
}
