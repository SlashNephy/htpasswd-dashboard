{
  "name": "htpasswd-dashboard",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/htpasswd-dashboard"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "hadolint": "hadolint Dockerfile",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "start": "next start"
  },
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ],
    "rules": {
      "import/extensions": "off",
      "@susisu/safe-typescript/no-type-assertion": "off",
      "@susisu/safe-typescript/no-unsafe-object-property-check": "off"
    }
  },
  "dependencies": {
    "@emotion/react": "11.11.3",
    "@emotion/server": "11.11.0",
    "@kubernetes/client-node": "0.19.0",
    "@mantine/core": "5.10.5",
    "@mantine/hooks": "5.10.5",
    "@mantine/next": "5.10.5",
    "@mantine/notifications": "5.10.5",
    "@tabler/icons-react": "2.38.0",
    "@tanstack/react-query": "4.35.7",
    "async-mutex": "0.4.1",
    "bcrypt": "5.1.1",
    "gravatar": "1.8.2",
    "http-status-codes": "2.3.0",
    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "3.0.1",
    "nanoid": "5.0.5",
    "next": "13.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-array-length": "0.1.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "2.1.4",
    "@slashnephy/prettier-config": "1.0.73",
    "@types/bcrypt": "5.0.0",
    "@types/gravatar": "1.8.4",
    "@types/jsonwebtoken": "9.0.5",
    "@types/node": "18.18.3",
    "@types/react": "18.2.51",
    "@types/react-dom": "18.2.18",
    "concurrently": "8.2.1",
    "eslint": "8.50.0",
    "prettier": "3.2.4",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@3.6.4"
}
